@model List<DemoApplication.Database.Models.Navbar>

@foreach (var navbar in @Model)
{
    <li class="menu-item has-children">

        <a href="@navbar.Url" style="font-weight : @(navbar.IsMain ? "bolder;" : "normal;" )"> @navbar.Name</a>
        <ul class="sub-menu">
            @foreach (var subnavbar in navbar.SubNavbars)
            {
                <li> <a href="@subnavbar.Url">@subnavbar.Name</a></li>
            }
        </ul>
    </li>

}